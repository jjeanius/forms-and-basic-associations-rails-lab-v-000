<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_name,  Genre.all, :id, :name, {:selected => :genre_name} %>


  <%= input_field :note_contents %>
  <%= input_field :note_contents %>

  <%= f.submit "commit" %>
<% end %>
