<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id,  Genre.all, :id, :name %>
  <%= f.label :song_note %>

  <%= f.submit "commit" %>
<% end %>
