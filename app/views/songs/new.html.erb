<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_name,  Genre.all, :id, :name, {:selected => :genre_name} %>


  <input_field :song_note_1 >
  <input_field :song_note_2 onerror="">

  <%= f.submit "commit" %>
<% end %>
